# Frontend Environment Variables for TechProjectsHub
# Copy this file to .env and update with your actual backend URL

# Backend API URL (replace with your deployed backend URL)
VITE_API_BASE_URL=https://your-backend-url.onrender.com/api/lms

# WebSocket URL for real-time features (replace with your deployed backend URL)
VITE_SOCKET_URL=https://your-backend-url.onrender.com

# App Configuration (optional)
VITE_APP_NAME=TechProjectsHub
VITE_APP_VERSION=1.0.0

# Development Settings (optional)
VITE_DEBUG_MODE=true
VITE_LOG_LEVEL=info

